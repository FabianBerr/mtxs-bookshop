{
  "name": "mtxs-bookshop",
  "version": "0.0.1",
  "description": "MTXS Bookshop",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^2.15.0",
    "@sap-cloud-sdk/http-client": "^2.15.0",
    "@sap-cloud-sdk/util": "^2.15.0",
    "@sap/cds": "^6",
    "@sap/cds-mtxs": "^1",
    "express": "^4",
    "hdb": "^0.19.0",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "start:hybrid": "cds build --production && cp gen/srv/resources.tgz . && cds bind --exec cds-serve --profile hybrid"
  },
  "cds": {
    "requires": {
      "multitenancy": true,
      "toggles": true,
      "extensibility": true,
      "[hybrid]": {
        "auth": {
          "kind": "mocked"
        },
        "db": {
          "kind": "hana-cloud"
        }
      },
      "[production]": {
        "db": {
          "kind": "hana-cloud"
        }
      },
      "db": {
        "kind": "sql"
      },
      "cfapi": {
        "kind": "rest",
        "credentials": {
          "destination": "CFAPI"
        },
        "destinationOptions": {
          "selectionStrategy": "alwaysProvider"
        }
      }
    }
  }
}
